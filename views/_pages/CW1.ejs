<!-- perticular page css -->

<%- contentFor('title') %>  
<title>Hoa Than Son</title>

<%- contentFor('breadcrumb') %>
<div class="col-sm-6">
    <div class="page-title-box">
        <h4 class="font-size-18">CW Information</h4>
        <ol class="breadcrumb mb-0">
            <li class="breadcrumb-item active">Welcome to HTS Market Information</li>
        </ol>
    </div>
</div>

<%- contentFor('body') %>

<div class="row">
  <div class="col-lg-12">  
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">CW</h4>
        <div class="table-responsive">
          <table class="table table-striped ">
            <thead class="text-center">
              <th>Ticker</th>
              <th>a</th>

            </thead>
            <tbody>
                      
              <% for(let i=0; i< result[0].length; i++) {%>
                <tr class="text-center">
                  <td><%= result[0][i].symbol %></a></td>
                  <td><%= result[0][i].underlying %></td>
                  
                </tr>
              <% } %>   

            </tbody>
          </table>

        </div>
      </div>
    </div>
  </div>
    
</div>

    
<!-- end row -->

